{
  "frontendPort": 4001,
  "backendPort": 4002,
  "apiBase": "/api",
  "backendUrl": "http://localhost:4002",
  "cors": {
    "origin": ["http://localhost:4001", "http://127.0.0.1:4001"],
    "credentials": true,
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
    "allowedHeaders": [
      "Content-Type",
      "Authorization",
      "X-Requested-With",
      "Accept",
      "Origin"
    ],
    "maxAge": 86400
  }
}

