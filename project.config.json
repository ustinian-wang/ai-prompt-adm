{
  "frontendPort": 3000,
  "backendPort": 8080,
  "apiBase": "/api",
  "backendUrl": "http://localhost:8080",
  "cors": {
    "origin": ["http://localhost:3000", "http://127.0.0.1:3000"],
    "credentials": true,
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "PATCH"],
    "allowedHeaders": [
      "Content-Type",
      "Authorization",
      "X-Requested-With",
      "Accept",
      "Origin"
    ],
    "maxAge": 86400
  }
}

